generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Hotel {
  id            String      @id @default(cuid())
  name          String
  destination   String
  driveUrl      String
  coverImageUrl String
  createdAt     DateTime    @default(now())
  updatedAt     DateTime    @updatedAt
}
